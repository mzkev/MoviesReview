@page
@model MoviesReviewApp.Pages.GalleryModel
@{
    ViewData["Title"] = "Gallery";
}

<h1>Gallery</h1>
<div role="tabpanel" class="tab-pane" id="onairtv">
    <div class="tabs-container">
        @foreach(var movie in Model.result.Take(6))
        {
         <div class="embed-responsive embed-responsive-16by9 z-depth-1-half">
                                    <iframe class="embed-responsive-item" src="@movie.id.videoURL"
                                            allowfullscreen frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" ></iframe>
                                </div>
        }
        </div>
</div>
